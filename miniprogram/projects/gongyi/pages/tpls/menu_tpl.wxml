<template name="menuTpl"> 
	<view class="tab-bar" >   
		<view wx:if="{{returnHome}}" class="tab-bar-home" bindtap="url" data-type="relaunch"
			data-url="/pages/home/index/home_index">
			<text class="tab-bar-home-text icon-homefill text-grey"></text>
		</view>

		<view class="tab-bar-border"></view>
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/gongyi/pages/default/index/default_index">
			<image class="tab-icon" src="/projects/gongyi/images/tabbar/home{{curMenu=='home_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='home_index'?'menu-project':''}}">首页</view>
		</view>

		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/gongyi/pages/news/cate1/news_cate1">
			<image class="tab-icon" src="/projects/gongyi/images/tabbar/news{{curMenu=='news_cate1'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='news_cate1'?'menu-project':''}}">公告通知</view>
		</view>

		<view wx:if="{{skin.cate2!==false}}" class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/gongyi/pages/enroll/index/enroll_index">
			<image class="tab-icon" src="/projects/gongyi/images/tabbar/enroll{{curMenu=='enroll_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='enroll_index'?'menu-project':''}}">报名</view>
		</view> 
	 
		<view class="tab-bar-item" bindtap="url" data-type="relaunch" data-url="/projects/gongyi/pages/my/index/my_index">
			<image class="tab-icon" src="/projects/gongyi/images/tabbar/my{{curMenu=='my_index'?'_cur':''}}.png"></image>
			<view class="tab-text {{curMenu=='my_index'?'menu-project':''}}">我的</view>
		</view>


	</view>
</template>